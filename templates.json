{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 1,
      "title": "Registry",
      "description": "Docker image registry",
      "categories": ["docker"],
      "platform": "linux",
      "logo": "https://ngochangtf.github.io/portainer-templates/assets/registry.png",
      "image": "registry:latest",
      "env": [
        {
          "name": "OTEL_TRACES_EXPORTER",
          "default": "none",
          "preset": true
        }
      ],
      "ports": ["5000/tcp"],
      "volumes": [
        {
          "container": "/etc/localtime",
          "bind": "/etc/localtime"
        },
        {
          "container": "/etc/timezone",
          "bind": "/etc/timezone"
        },
        {
          "container": "/var/lib/registry"
        }
      ]
    },
    {
      "id": 2,
      "type": 1,
      "title": "PhpMyAdmin",
      "description": "A web interface for MySQL and MariaDB",
      "categories": ["management"],
      "platform": "linux",
      "logo": "https://ngochangtf.github.io/portainer-templates/assets/phpmyadmin.png",
      "image": "phpmyadmin:latest",
      "env": [
        {
          "name": "PMA_HOSTS",
          "default": "172.17.0.1"
        },
        {
          "name": "PMA_PORT",
          "default": "3306"
        }
      ],
      "ports": ["80/tcp"],
      "volumes": [
        {
          "container": "/etc/localtime",
          "bind": "/etc/localtime"
        },
        {
          "container": "/etc/timezone",
          "bind": "/etc/timezone"
        }
      ]
    },
    {
      "id": 3,
      "type": 1,
      "title": "Redis Insight",
      "description": "Redis Insight - our best official GUI for Redis",
      "categories": ["management"],
      "platform": "linux",
      "logo": "https://ngochangtf.github.io/portainer-templates/assets/redisinsight.png",
      "image": "redis/redisinsight:latest",
      "env": [
        {
          "name": "RI_REDIS_HOST",
          "default": "172.17.0.1"
        },
        {
          "name": "RI_REDIS_PORT",
          "default": "6379"
        },
        {
          "name": "RI_REDIS_USERNAME",
          "default": "default"
        },
        {
          "name": "RI_REDIS_PASSWORD",
          "default": ""
        }
      ],
      "ports": ["5540/tcp"],
      "volumes": [
        {
          "container": "/etc/localtime",
          "bind": "/etc/localtime"
        },
        {
          "container": "/etc/timezone",
          "bind": "/etc/timezone"
        }
      ]
    }
  ]
}
